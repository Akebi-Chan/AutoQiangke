<Application
    x:Class="AutoQiangke.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:AutoQiangke" ShutdownMode="OnExplicitShutdown"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <materialDesign:BundledTheme
                    BaseTheme="Light"
                    PrimaryColor="Blue"
                    SecondaryColor="Lime" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
                <ResourceDictionary Source="Styles/Window.Universal.xaml" />
                <ResourceDictionary Source="Styles/Dictionary1.xaml" />
                <ResourceDictionary Source="Styles/TaskListStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <sys:String x:Key="xklcmc">未知阶段</sys:String>    <!--选课阶段描述-->
            <sys:String x:Key="xkxnmc">未知</sys:String>        <!--选课学年描述-->
            <sys:String x:Key="xkxqmc">未知</sys:String>        <!--选课学期描述-->
            <sys:String x:Key="stuname">未登录</sys:String>
            <sys:String x:Key="Time">Ready</sys:String>
        </ResourceDictionary>
    </Application.Resources>
</Application>
